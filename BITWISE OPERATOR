public class bitwise

{
    public static void main(String args[])

    {
        int a=5;
         int b=4;
        System.out.println("AND" +" " + (a&b));
        System.out.println("OR" +" "+ (a|b));
        System.out.println("XOR" +" "+ (a^b));
        System.out.println("COMPLEMENT" +" "+ (~a));
        
        
    }
}
